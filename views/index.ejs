<%- include header %>
        <!-- 待编辑部分 -->
        <div class="container">
        	<div class="row">
        	<% arts.forEach(function(art){%>
        		<div class="col-xs-8 col-xs-offset-2">
        			<div class="article-box">
        				<a href="/default/<%= art._id_%>">
        					<h3 class="text-center"><%= art.title %></h3>
        				</a>
        				<p class="text-center">
        					<span class="label label-info"><%= art.tag %></span>
        					<span class="create-time">
        						<span class="text-bold">时间:</span><%= art.createTime %>
        					</span>
        				</p>
        				<p><%= art.content %></p>
        			</div>
        		</div>
        	<%})%>
        	</div>
        </div>
    </body>
</html>